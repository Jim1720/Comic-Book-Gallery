@{
    Layout = null;

    var seriesTitle = "The Amazing Spider Man";
    var issueNumber = 700;
    var description = "<p>Final Issue </p>";
    var artists = new string[]
    {
        "Script: DanSlot",
        "Pencils: Ramos"
    };


}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <h1>@seriesTitle</h1>

        <h2>Issue:@issueNumber</h2>

        <h5>Description</h5>
        <div>@Html.Raw(@description)</div>
        
        @if(artists.Length > 0 )
        {
            <h5>Artists:</h5>
            <ul>
            @foreach(var artist in artists)
            {
                <li><text>@artist</text></li>

            }


            </ul>
        }


    </div>
</body>
</html>
